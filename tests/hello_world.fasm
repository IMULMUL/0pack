format ELF64 executable
; rax, rdi, rsi, rdx, rcx, r8, r9, r10, r11 are scratch registers
; rbx, rsp, rbp, r12, r13, r14, and r15 are preserved registers


jmp afterpayload

payload db 'Hello Payload!!', 0xA, 0
sizeOfString = $-payload
afterpayload:

mov rax, 1
mov rdi, 1
mov rsi, payload
mov rdx, sizeOfString ; length
syscall

